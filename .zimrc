# Load any helper scripts as defined here
zmodules=(completion archive autosuggestions directory environment fasd git \
          git-info history syntax-highlighting history-substring-search \
          input prompt ssh utility)

# Input
zinput_mode='vi'
zdouble_dot_expand='true'

# Prompt
zprompt_theme='gitster'

# Syntax-Highlighting
zhighlighters=(main brackets cursor root)

# SSH
# Load these ssh identities
zssh_ids=(id_rsa)
